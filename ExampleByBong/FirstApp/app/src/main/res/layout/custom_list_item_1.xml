<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
     android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="20dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="이름입니다"
        android:id="@+id/textView_customList_name"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="나이 "
        android:id="@+id/textView_customList_age"
        android:layout_below="@+id/textView_customList_name"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="18sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="주소입니다"
        android:id="@+id/textView_customList_address"
        android:layout_alignTop="@+id/textView_customList_name"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="16sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textView_customList_phoneNum"
        android:layout_alignBottom="@+id/textView_customList_age"
        android:layout_alignRight="@+id/textView_customList_address"
        android:layout_alignEnd="@+id/textView_customList_address" />
</RelativeLayout>